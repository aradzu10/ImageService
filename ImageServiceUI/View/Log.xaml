<UserControl x:Class="ImageServiceUI.View.Log"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ImageServiceUI.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
        <Grid Background="White">
            <Label Content="Log" FontSize="36" FontWeight="Bold" Foreground="#FF2F41B0" RenderTransformOrigin="1.709,2.431" HorizontalAlignment="Center" Canvas.Left="309" VerticalAlignment="Top"/>
            <DataGrid x:Name="LogGrid" AutoGenerateColumns="False" Margin="10,63,10,10">
                <DataGrid.Columns>
                    <DataGridTemplateColumn Header="Type" Width="70" IsReadOnly="True" CanUserResize="False"/>
                    <DataGridTemplateColumn Header="Message" Width="*" IsReadOnly="True"/>
            </DataGrid.Columns>
            </DataGrid>
        </Grid>
</UserControl>
